funcao int achar (int n, carac c)
var {
	logic (termino)
	int (i, j)
}

comandos {
	termino = falso;
	i = 0;
	j = ~1;
	enquanto (i < n && !termino) {
		se (vector[i] == c) {
			termino = verdade;
			j = i;
		}
		senao 
			termino = falso;
		i = i + 1;
	}
	retornar j;
}

